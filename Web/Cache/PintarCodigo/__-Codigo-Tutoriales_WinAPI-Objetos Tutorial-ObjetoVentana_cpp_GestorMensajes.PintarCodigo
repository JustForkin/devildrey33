tr(GetWindowLongPtr(nhWnd, GWL_USERDATA))</span></div><div l=17><span class='TxtNegro'>            );</span></div><div l=18><span class='TxtNegro'>            <span class='TxtAzul'>if</span> (Ventana != NULL) {</span></div><div l=19><span class='TxtNegro'>                LRESULT Ret = Ventana-&gt;GestorMensajes(uMsg, wParam, lParam);</span></div><div l=20><span class='TxtNegro'>                <span class='TxtAzul'>if</span> (Ret == USAR_GESTOR_POR_DEFECTO) Ret = DefWindowProc(nhWnd, uMsg, wParam, lParam);</span></div><div l=21><span class='TxtNegro'>                <span class='TxtAzul'>return</span> Ret;</span></div><div l=22><span class='TxtNegro'>            }</span></div><div l=23><span class='TxtNegro'>            <span class='TxtAzul'>return</span> DefWindowProc(nhWnd, uMsg, wParam, lParam);</span></div><div l=24><span class='TxtNegro'>        }</span></div><div l=25><span class='TxtNegro'>    }</span></div><div l=26><span class='TxtNegro'>};</span></div></pre></div>