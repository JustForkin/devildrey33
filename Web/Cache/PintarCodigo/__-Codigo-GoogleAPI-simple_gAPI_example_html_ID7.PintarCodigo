<div class='Codigo_Marco' id='ID7'><div class='Codigo_Titulo'>Manual request with CORS</div><div class='Codigo_Numeracion'>1<br>2<br>3<br>4<br>5<br>6<br>7<br>8<br>9<br>10<br>11<br>12<br>13<br>14<br>15<br>16<br>17<br>18<br>19<br>20<br>21<br></div><pre class='Codigo_PRECodigo'><div l=1><span class='TxtNegro'><span class='TxtAzul'>function</span> method_3_request() {</span></div><div l=2><span class='TxtNegro'>    <span class='TxtLila'>$</span>(<span class='TxtAmarillo'>'body'</span>).append(<span class='TxtAmarillo'>'&lt;p&gt;Launching a manual request with CORS&lt;/p&gt;'</span>);</span></div><div l=3><span class='TxtNegro'>    gapi.auth.authorize({</span></div><div l=4><span class='TxtNegro'>            client_id    : clientId,</span></div><div l=5><span class='TxtNegro'>            scope        : scopes,</span></div><div l=6><span class='TxtNegro'>            immediate    : true},</span></div><div l=7><span class='TxtNegro'>            <span class='TxtAzul'>function</span>() {</span></div><div l=8><span class='TxtNegro'>                <span class='TxtAzul'>var</span> oauthToken = gapi.auth.getToken();</span></div><div l=9><span class='TxtNegro'>                <span class='TxtAzul'>var</span> xhr = <span class='TxtAzul'>new</span> XMLHttpRequest();</span></div><div l=10><span class='TxtNegro'>                <span class='TxtAzul'>var</span> url_request = <span class='TxtAmarillo'>'https://www.googleapis.com/drive/v2/files?maxResults=10'</span>;</span></div><div l=11><span class='TxtNegro'>                                  <span class='TxtGris'>// + '&access_token=' + encodeURIComponent(oauthToken.access_token);</span></span></div><div l=12><span class='TxtNegro'>                <span class='TxtLila'>$</span>(<span class='TxtAmarillo'>'body'</span>).append(<span class='TxtAmarillo'>'&lt;p&gt;xhr: '</span> + url_request + <span class='TxtAmarillo'>'&lt;/p&gt;'</span>);</span></div><div l=13><span class='TxtNegro'>                xhr.open(<span class='TxtAmarillo'>'GET'</span>, url_request);</span></div><div l=14><span class='TxtNegro'>                xhr.setRequestHeader(<span class='TxtAmarillo'>'Authorization'</span>, <span class='TxtAmarillo'>'Bearer '</span> + oauthToken.access_token);</span></div><div l=15><span class='TxtNegro'>                xhr.onload = <span class='TxtAzul'>function</span>() {</span></div><div l=16><span class='TxtNegro'>                    <span class='TxtAzul'>var</span> obj_res = JSON.parse(xhr.responseText);</span></div><div l=17><span class='TxtNegro'>                    load_result(obj_res);</span></div><div l=18><span class='TxtNegro'>                };</span></div><div l=19><span class='TxtNegro'>                xhr.send();</span></div><div l=20><span class='TxtNegro'>            });</span></div><div l=21><span class='TxtNegro'>}</span></div></pre></div>