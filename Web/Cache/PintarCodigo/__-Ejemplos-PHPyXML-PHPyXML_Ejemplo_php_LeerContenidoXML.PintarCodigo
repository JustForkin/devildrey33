<div class='Codigo_Marco' id='EjemploPHP'><div class='Codigo_Titulo'>Funci√≥n PHP <b>LeerContenidoXML</b></div><div class='Codigo_Numeracion'>1<br>2<br>3<br>4<br>5<br>6<br>7<br>8<br>9<br></div><pre class='Codigo_PRECodigo'><div l=1><span class='TxtNegro'><span class='TxtAzul'>function</span> LeerContenidoXML(<span class='TxtMarron'>$Archivo</span>) {</span></div><div l=2><span class='TxtNegro'>    <span class='TxtMarron'>$ArchivoLista</span> = <span class='TxtAzul'>fopen</span>(<span class='TxtMarron'>$Archivo</span>, <span class='TxtAmarillo'>"r"</span>, <span class='TxtAzul'>true</span>);</span></div><div l=3><span class='TxtNegro'>    <span class='TxtMarron'>$DatosArchivo</span> = <span class='TxtAzul'>fread</span>(<span class='TxtMarron'>$ArchivoLista</span>, <span class='TxtAzul'>filesize</span>(<span class='TxtMarron'>$Archivo</span>));</span></div><div l=4><span class='TxtNegro'>    <span class='TxtAzul'>fclose</span>(<span class='TxtMarron'>$ArchivoLista</span>);</span></div><div l=5><span class='TxtNegro'>    <span class='TxtMarron'>$Parser</span> = <span class='TxtAzul'>xml_parser_create</span>();</span></div><div l=6><span class='TxtNegro'>    <span class='TxtAzul'>xml_parse_into_struct</span>(<span class='TxtMarron'>$Parser</span>, <span class='TxtMarron'>$DatosArchivo</span>, <span class='TxtMarron'>$Valores</span>, <span class='TxtMarron'>$Indices</span>);</span></div><div l=7><span class='TxtNegro'>    <span class='TxtAzul'>xml_parser_free</span>(<span class='TxtMarron'>$Parser</span>);</span></div><div l=8><span class='TxtNegro'>    <span class='TxtAzul'>return</span> <span class='TxtMarron'>$Valores</span>;</span></div><div l=9><span class='TxtNegro'>}</span></div></pre></div>