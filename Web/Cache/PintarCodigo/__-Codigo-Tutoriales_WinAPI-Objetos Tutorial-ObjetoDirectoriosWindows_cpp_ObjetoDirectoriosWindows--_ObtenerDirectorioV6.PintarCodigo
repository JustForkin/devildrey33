<div class='Codigo_Marco' id='ID_ObtenerDirectorioV6'><div class='Codigo_Titulo'>Archivo : ObjetoDirectoriosWindows.cpp</div><div class='Codigo_Numeracion'>1<br>2<br>3<br>4<br>5<br>6<br>7<br>8<br>9<br>10<br>11<br>12<br>13<br>14<br>15<br></div><pre class='Codigo_PRECodigo'><div l=1><span class='TxtNegro'><span class='TxtVerde'>// -Función que llamara a SHGetKnownFolderPath si es posible</span></span></div><div l=2><span class='TxtNegro'><span class='TxtVerde'>//  NOTA : StrOut debe ser un TCHAR de tamaño MAX_PATH como minimo</span></span></div><div l=3><span class='TxtNegro'>BOOL ObjetoDirectoriosWindows::_ObtenerDirectorioV6(<span class='TxtAzul'>const</span> GUID& rfid, TCHAR *StrOut) {</span></div><div l=4><span class='TxtNegro'>    <span class='TxtAzul'>#if </span>_MSC_VER &gt;</span> <span class='TxtRojo'>1300</span></div><div l=5><span class='TxtNegro'>        <span class='TxtAzul'>if</span> (_SHGetKnownFolderPath != NULL) { <span class='TxtVerde'>// Vista o superior</span></span></div><div l=6><span class='TxtNegro'>            PWSTR Tmp = NULL;</span></div><div l=7><span class='TxtNegro'>            <span class='TxtAzul'>if</span> (S_OK == _SHGetKnownFolderPath(FOLDERID_ProgramData, NULL, <span class='TxtRojo'>0</span>, &Tmp)) {</span></div><div l=8><span class='TxtNegro'>                wcscpy_s(StrOut, MAX_PATH, Tmp);</span></div><div l=9><span class='TxtNegro'>                CoTaskMemFree(Tmp);</span></div><div l=10><span class='TxtNegro'>                <span class='TxtAzul'>return</span> TRUE;</span></div><div l=11><span class='TxtNegro'>            }</span></div><div l=12><span class='TxtNegro'>        }</span></div><div l=13><span class='TxtNegro'>    <span class='TxtAzul'>#endif</span></span></div><div l=14><span class='TxtNegro'>    <span class='TxtAzul'>return</span> FALSE;</span></div><div l=15><span class='TxtNegro'>}</span></div></pre></div>